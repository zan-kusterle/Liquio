{
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "background/index.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content/index.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "https://ajax.googleapis.com/"
  ],
  "name": "Liquio",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:3001; object-src 'self'; "
}